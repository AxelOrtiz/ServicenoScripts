    data.map(function(item, index){
    var question = new GlideRecord('question_choice');
    question.initialize();
    question.setValue('rec_misc','0.00'); // default value dado por snow
    question.setValue('misc','0.00'); // deafault value dado por snow
    question.setValue('order',(index+1) + '00'); // aquí lo que hace es aumentar el orden de 100 en 100 tomando en consideración el index, que se le suma 1 para que empiece en 1
    question.setValue('question','816ba0821b0bf51077496576bc4bcbb5'); //Cambia por sysid de tu variable
    question.setValue('text',item);
    question.setValue('value',item.toLowerCase().replaceAll(' ', '_')
    .replaceAll('+', '_')
    .replaceAll(')', '_')
    .replaceAll(':', '_')
    .replaceAll('-', '_')
    .replaceAll('(', '')); // aqui lo que se hace es que se contruye el value con base el value original se conviergte a minusculas todo el texto y remplaza los espacios con underscore
    question.setValue('inactive',false);
    question.insert();
    });
